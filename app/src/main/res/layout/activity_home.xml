<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_kotoba"
    tools:context=".ui.home.HomeActivity">

    <Button
        android:id="@+id/btnMulai"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginStart="146dp"
        android:layout_marginTop="259dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="28dp"
        android:text="@string/start"
        app:layout_constraintBottom_toTopOf="@+id/btnInstruksi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnInstruksi"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:layout_marginStart="146dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="391dp"
        android:text="@string/instruksi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMulai" />

    <FrameLayout
        android:id="@+id/frame_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>